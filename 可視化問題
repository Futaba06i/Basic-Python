    {
      "cell_type": "code",
      "source": [
        "scatter_data = pd.read_csv(\"https://raw.githubusercontent.com/Futaba06i/Basic-Python/master/colab/quizzes/matplotlib_quiz/scatter_data.csv\")\n",
        "x1 = scatter_data[scatter_data[\"class\"] == 0]\n",
        "y1 = scatter_data[scatter_data[\"class\"] == 0]\n",
        "x2 = scatter_data[scatter_data[\"class\"] == 1]\n",
        "y2 = scatter_data[scatter_data[\"class\"] == 1]\n",
        "x3 = scatter_data[scatter_data[\"class\"] == 2]\n",
        "y3 = scatter_data[scatter_data[\"class\"] == 2]\n",
        "plt.xlabel(\"length(cm)\")\n",
        "plt.ylabel(\"width(cm)\")\n",
        "plt.title(\"クラス0～2のlengthとwidthに関する散布図\")\n",
        "plt.scatter(x1, y1, color = \"red\", label = \"class0\", marker=\"o\")\n",
        "plt.scatter(x2, y2, color = \"blue\", label = \"class1\",marker=\"o\")\n",
        "plt.scatter(x3, y3, color = \"green\", label = \"class2\",marker=\"o\")\n",
        "plt.legend(loc= \"upper right\")\n",
        "plt.grid()\n",
        "plt.show()"
      ],
