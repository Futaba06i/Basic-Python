        "line_data = pd.read_csv(\"https://raw.githubusercontent.com/Futaba06i/Basic-Python/master/colab/quizzes/matplotlib_quiz/line_data.csv\")\n",
        "plt.subplot(2,1,1)\n",
        "x = pd.Series(line_data[\"期間\"], dtype=\"object\")\n",
        "y = pd.Series(line_data[\"件数\"], dtype=\"int\")\n",
        "plt.title(\"期間ごとの件数の推移\")\n",
        "plt.xlabel(\"期間\")\n",
        "plt.ylabel(\"件数\")\n",
        "plt.grid(True)\n",
        "plt.plot(x, y,marker=\"o\" )\n",
        "plt.subplot(2,1,2)\n",
        "plt.title(\"期間ごとの費用の推移\")\n",
        "plt.xlabel(\"期間\")\n",
        "plt.ylabel(\"費用（円)\")\n",
        "plt.grid(True)\n",
        "plt.bar(x, y)\n",
        "plt.show()"
      ],
